// Desenvolva um algoritmo que leia dois números inteiros e mostre o somatório entre eles.

programa
{
	
	funcao inicio()
	{
		inteiro a, b
		inteiro soma


		escreva("Digite um valor")
		leia(a)

		escreva("Digite outro valor")
		leia(b)

		soma = a + b

		escreva("A soma entre " + a + " e " + b + " é igual a " + soma + ".")

		
	}
}
